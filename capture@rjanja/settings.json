{
   "camera-program": "cinnamon",
   "recorder-program": "cinnamon",
   "include-cursor": true,
   "open-after": false,
   "delay-seconds": 5,
   "camera-save-dir": "",
   "recorder-save-dir": "",
   "camera-save-prefix": "screenshot-%TYPE-%Y-%M-%D-%H%I%S",
